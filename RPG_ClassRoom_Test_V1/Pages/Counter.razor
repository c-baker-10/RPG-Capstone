@page "/counter"
@using Objects

<PageTitle>Counter</PageTitle>

<h1>Counter</h1>

<p role="status">Current count: @currentCount</p>

<button class="btn btn-primary" @onclick="IncrementCount">Press to Count</button>

@code {
    private int currentCount = 15;

    private void IncrementCount()
    {
        currentCount++;
    }
}
